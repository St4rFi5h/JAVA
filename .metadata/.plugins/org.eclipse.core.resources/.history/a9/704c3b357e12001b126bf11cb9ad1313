package text06.generics08;
class Box<T>{
	private T ob;
	public void set(T o) {
		ob=o;
	}
	public T get() {
		return ob;
	}
}

	class BoxFactory{
		public static <T>Box<T>makeBox(T o){
			Box<T> box = new Box<T>();
			box.set(o);
			return box;
		}
	}

public class GenericMethodBoxMaker {
	public static void main(String[] args) {
		Box<String>sBox = BoxFactory.makeBox("sweet");
		Syste.out.println(sBox.get());
		Syste.out.println(dBox.get());

	}

}
