package com.threadEx;

public class Real extends Thread
{


	public void run()
	{
		// 인터럽트 됬을때 예외처리
		try
		{
			for(int i = 0 ; i < 10 ; i++)
			{
				// 스레드 0.5초동안 대기
				Thread.sleep(100);
				System.out.println("나는바보다");
			}
		}catch(InterruptedException e)
		{
			System.out.println(e);
		}
	}




}