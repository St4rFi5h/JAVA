import java.util.ArrayList;
import java.util.Scanner;
public class Ex05 {

	public static void main(String[] args) {
		while(true) {
			ArrayList<Info> list = new ArrayList<Info>();
			Scanner scan = new Scanner(System.in);

			System.out.println("번호누르세요 1 , 2");
			int a = scan.nextInt();
			if (a== 1) {

				Info in = new Info ();
				in.name = "hanna";
				in.number = 1234;

				list.add(in); 



				Info in1 = new Info ();
				in1.name = "han2na";
				in1.number = 1233234;

				list.add(in1); 

			}

			if(a==2) {
				System.out.println (list.get(0)); 
				System.out.println (list.get(0).name);
				System.out.println (list.get(0).name.toString());
				System.out.println (list.get(0).name.hashCode());

				System.out.println ("");
				System.out.println (list.get(0).number);
				System.out.println (list.get(1).name);
			}
		}

	}
}