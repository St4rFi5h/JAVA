import java.util.Scanner;
class Sample {
	
	String name = "없음";
	Scanner scan = new Scanner(System.in);
}
public class Pro01 {

	public void Input() {
		Scanner scan = new Scanner(System.in);
		System.out.println("학생 이름을 입력해주세요.>>");
		String name=scan.next();
	}
	
	public void Print() {
	
		System.out.println("학생이름: " + name);
	}
	
	
	public static void main(String[] args) {

		Sample s = new Sample();
		s.Input();
		s.Print();
		
	
	

	}

}
