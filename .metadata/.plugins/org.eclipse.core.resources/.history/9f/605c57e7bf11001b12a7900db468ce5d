package com.minigame;
import java.util.Scanner;

public class If_Elseif {
	
	public static void main(String[] args) {
		
		Scanner sc =new Scanner (System.in);
		
		int i = sc.nextInt();
		
		if(i%2 ==0) {
			System.out.println("2의 배수입니다.");
		}else if (i%3==0) {
			System.out.println("3의 배수입니다.");
		}
	}
}
