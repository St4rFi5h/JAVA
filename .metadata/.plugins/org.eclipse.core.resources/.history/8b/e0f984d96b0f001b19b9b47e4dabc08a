class People {
   public String name;
   public  String  number;

   public People () {
      this.name = name;
      this.number = number;
   }
}






   public class Student extends People {
      public int studentNo;

      public void prtInfo () {
         System.out.println ("name: " + name);
         System.out.println ("number: " + number); 
         
      }
     
      public Student ( String number,String name, int studentNo ) {
         super (name, number);
         this.studentNo = studentNo;
      }

      
      
      
      
      
      
      
      
      
      
   }
