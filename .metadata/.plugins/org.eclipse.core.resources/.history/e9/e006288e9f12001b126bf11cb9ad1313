
public class Arraytest2 {


	///x 자로 더한 값 
	public static void main(String[] args) {
		int[][] a = new int[5][5];
		int x =1;
		int sum = 0;
		for(int i = 0; i < 5; i++) {
			for(int j = 0; j < 5; j++) {
				if(i)
				a[i][j] = x;
				x++;

			}
			x=1;
		}
		for(int i = 0; i < 5; i++) {
			for(int j = 0; j < 5; j++) {
				System.out.printf("%4d",a[i][j]);
			}
			System.out.println("");
		}

	

	}
}
